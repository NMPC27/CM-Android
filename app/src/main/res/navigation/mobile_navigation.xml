<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.cinem.ui.map.MapFragment"
        android:label="@string/app_name" />

    <fragment
        android:id="@+id/navigation_cinemas"
        android:name="com.example.cinem.ui.cinemas.CinemasFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_cinemas" />

    <fragment
        android:id="@+id/navigation_tickets"
        android:name="com.example.cinem.ui.tickets.TicketsFragment"
        android:label="@string/app_name" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.cinem.ui.notifications.NotificationsFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_notifications" />
</navigation>